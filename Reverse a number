import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner pv=new Scanner(System.in);
        int a=pv.nextInt(),b,c=0;
        if(a>0){
            while(a>0){
                b=a%10;
                c=(c*10)+b;
                a=a/10;
            }System.out.println(c);
        }else 
            System.out.println("Invalid Input");
    }
}
