import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);      
        int a=s.nextInt();
        int b=s.nextInt();
        int c=s.nextInt();
        int d=s.nextInt();
        float e=((float)(a+c)/2);
        float g=((float)(b+d)/2);
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",e,g);
    }
}
