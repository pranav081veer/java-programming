import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ob= new Scanner(System.in);
        int n=ob.nextInt();
        int temp=n;
        int sum=0;
        
        while(temp>0){
        int digit= temp%10;
        sum= (sum*10)+digit;
        temp=temp/10;
        }
        if (sum==n)
            System.out.println("palindrome");
        else if(n<100)
            System.out.println("Invalid Input");
        else{
            System.out.println("not palindrome");
        }
        
    }
}
