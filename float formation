import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        float a=s.nextFloat();
        System.out.printf("%.6f\n",a);
         System.out.printf("%.4f\n",a);
         System.out.printf("%.2f\n",a);
         System.out.printf("%.0f\n",a);
    }
}
