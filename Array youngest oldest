import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner pv=new Scanner(System.in);
        int size=pv.nextInt();
        int arr[]=new int[size];
        for(int i=0;i<size;i++){
            arr[i]=pv.nextInt();
                    }
        Arrays.sort(arr);
    System.out.println( "Youngest="+arr[0]);
        System.out.println("Oldest="+arr[size-1]);
}}
