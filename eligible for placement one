import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        int a1=sc.nextInt();
        int a2=sc.nextInt();
        System.out.println("Name of the Student:"+a);
        if(a1==1&& a2>70){
            System.out.println(a+" is Eligible for Placement");
        }else if(a1==2&& a2>75){
            System.out.println(a+" is Eligible for Placement");
        }else{
                System.out.println(a+" is Not Eligible for Placement");
            }
        
    }
}
